
<div ng-controller="MainController">
  <div automatch-swipe class="card" ng-repeat="car in cars track by car.id">
    <div class="price-tag">{{ car.price }}</div>
    <div class="car-image" ng-style="{ 'background-image': 'url(' + getImage(car) + ')' }">
      <div class="car-title">
        {{ car.title }}
      </div>
    </div>
    <a ng-attr-href="{{ car.url }}" class="buy-button">BUY!</a>
  </div>
  <div class="sidebar">
    <ul>
    <li ng-repeat="item in items">
        <p>Name: </p>
        <p>Cost: </p>
        <slider floor="100" ceiling="1000" step="50" precision="2" ng-model="item.cost"></slider>
    </li>
    </ul>
  </div>

</div>
