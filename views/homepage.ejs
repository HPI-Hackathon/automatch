
<div ng-controller="MainController">
  <div automatch-swipe
     like="like(car)"
     dislike="dislike(car)"
     class="card"
     ng-style="{ 'z-index': $length - $index }"
     ng-repeat="car in cars track by car.id">
    <div class="price-tag">{{ car.price }}</div>
    <div class="car-image" ng-style="{ 'background-image': 'url(' + getImage(car) + ')' }">
      <div class="car-title">
        {{ car.title }}
      </div>
    </div>
    <a ng-show="$index == 0"
       ng-attr-href="{{ car.url }}"
       class="icon-money-1 buy-button"> BUY!</a>

    <div ng-show="$index == 0"
       class="fav-button icon-heart"></div>

  </div>
  <div class="automatchSidebar">

  </div>

</div>
